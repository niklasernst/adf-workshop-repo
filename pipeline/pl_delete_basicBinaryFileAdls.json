								"value": "@pipeline().parameters.par_directory",
								"type": "Expression"
							},
							"par_filename": {
								"value": "@pipeline().parameters.par_filename",
								"type": "Expression"
							}
						}
					},
					"enableLogging": false,
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
